# =====================================================
# ENVIRONMENT CONFIGURATION TEMPLATE
# =====================================================
# Copy this file to .env and fill in your values
# =====================================================

# =====================================================
# BLOG INTEGRATION (Optional)
# =====================================================
# Webhook URL for fetching blog posts (Dev.to format expected)
# Leave empty to use static data instead
REACT_APP_BLOG_WEBHOOK_URL=

# Example for Dev.to API:
# REACT_APP_BLOG_WEBHOOK_URL=https://dev.to/api/articles?username=yourusername

# Example for custom webhook:
# REACT_APP_BLOG_WEBHOOK_URL=https://your-webhook-url.com/api/blogs

# =====================================================
# PROJECTS INTEGRATION (Optional)
# =====================================================
# Webhook URL for fetching GitHub projects
# Leave empty to use static data instead
REACT_APP_PROJECT_WEBHOOK_URL=

# Example for GitHub API:
# REACT_APP_PROJECT_WEBHOOK_URL=https://api.github.com/users/yourusername/repos

# Example for custom webhook:
# REACT_APP_PROJECT_WEBHOOK_URL=https://your-webhook-url.com/api/projects

# =====================================================
# ANALYTICS & TRACKING (Optional)
# =====================================================
# Google Analytics ID (if you want to track visitors)
# REACT_APP_GA_ID=G-XXXXXXXXXX

# =====================================================
# CONTACT FORM (Optional)
# =====================================================
# Email service endpoint for contact form
# REACT_APP_CONTACT_ENDPOINT=

# =====================================================
# FEATURE FLAGS (Optional)
# =====================================================
# Enable/disable certain features
# REACT_APP_ENABLE_3D_ANIMATIONS=true
# REACT_APP_ENABLE_CONTACT_FORM=true
